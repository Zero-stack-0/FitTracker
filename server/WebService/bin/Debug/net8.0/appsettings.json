{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoConfig": {
    "Collections": {
      "Users": "Users",
      "UserInformation": "UserInformation",
      "AiPrompt": "ai-prompts",
      "AiResponse": "AiResponse",
      "FitnessAndNutritionPlan": "FitnessAndNutritionPlan",
      "FoodMacros": "food-macros",
      "UserFoodLog": "user-food-log",
      "BasicFitnessPlan": "basic-fitness-plan"
    }
  },
  "AllowedHosts": "*",
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 50
      },
      {
        "Endpoint": "*:/api/UserFoodLog/add",
        "Period": "1m",
        "Limit": 30
      }
    ]
  }
}